{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jquery": true,
      "webextensions": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": [
        "error", 
        {
        "endOfLine": "auto"
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".jsx", ".ts", ".tsx"]
        }
      }
    }
}